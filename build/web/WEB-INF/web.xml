<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>StudentReg</servlet-name>
        <servlet-class>com.mycompany.college_notice_board.StudentReg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentLog</servlet-name>
        <servlet-class>com.mycompany.college_notice_board.StudentLog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLog</servlet-name>
        <servlet-class>com.mycompany.college_notice_board.AdminLog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HelpDesk</servlet-name>
        <servlet-class>com.mycompany.college_notice_board.HelpDesk</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DataConnection</servlet-name>
        <servlet-class>com.mycompany.college_notice_board.DataConnection</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadServlet</servlet-name>
        <servlet-class>com.mycompany.college_notice_board.UploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownloadServlet</servlet-name>
        <servlet-class>com.mycompany.college_notice_board.NoticeDownload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentDetailsEdit</servlet-name>
        <servlet-class>com.mycompany.college_notice_board.StudentDetailsEdit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DB</servlet-name>
        <servlet-class>com.mycompany.college_notice_board.DB</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentReg</servlet-name>
        <url-pattern>/StudentReg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentLog</servlet-name>
        <url-pattern>/StudentLog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLog</servlet-name>
        <url-pattern>/AdminLog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelpDesk</servlet-name>
        <url-pattern>/HelpDesk</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataConnection</servlet-name>
        <url-pattern>/DataConnection</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadServlet</servlet-name>
        <url-pattern>/UploadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadServlet</servlet-name>
        <url-pattern>/DownloadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentDetailsEdit</servlet-name>
        <url-pattern>/StudentDetailsEdit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DB</servlet-name>
        <url-pattern>/DB</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
